type UserData = 
 record {
   points: nat;
   referralCode: text;
   referralCount: nat;
   referredBy: opt principal;
 };
type Result_1 = 
 variant {
   err: text;
   ok: text;
 };
type Result = 
 variant {
   err: text;
   ok: UserData;
 };
service : {
  getReferralCode: () -> (Result_1);
  getUserData: () -> (Result);
}
